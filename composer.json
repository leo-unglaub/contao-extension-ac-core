{
	"name":"leo-unglaub/contao-ac-core",
	"description":"A auto completion framework for input fields",
	"keywords":["contao", "autocompletion", "ajax"],
	"type":"contao-module",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Leo Unglaub",
			"email":"leo@leo-unglaub.net",
			"homepage":"http://www.leo-unglaub.net",
			"role":"Developer"
		}
	],
	"support": {
		"email": "leo@leo-unglaub.net",
		"issues": "https://bitbucket.org/leo-unglaub/contao-ac-core/issues",
		"wiki": "https://www.leo-unglaub.net/software/contao-extensions",
		"irc": "irc://irc.freenode.org/contao.dev",
		"source": "https://bitbucket.org/leo-unglaub/contao-ac-core/src/"
	},
	"require":{
		"php":">=5.3",
		"contao/core":"~2",
                "contao-community-alliance/composer-installer": "*"
	},
	"autoload":{
		"classmap":["system/"]
	},
	"replace": {
		"contao-legacy/ac_core": "*"
	},
	"extra":{
		"contao": {
			"sources":{
				"system/modules/ac_core": "system/modules/ac_core"
			}
		}
	}
}

